{
    "arch": "nvptx",
    "cpu": "sm_20",
    "data-layout": "e-p:32:32-i64:64-i128:128-v16:16-v32:32-n16:32:64",
    "linker": "legacy-ptx-linker",
    "linker-flavor": "ld",
    "linker-is-gnu": true,
    "dll-prefix": "",
    "dll-suffix": ".ptx",
    "exe-prefix": "",
    "exe-suffix": ".ptx",
    "executables": true,
    "dynamic-linking": true,
    "llvm-target": "nvptx-nvidia-cuda",
    "max-atomic-width": 0,
    "os": "cuda",
    "obj-is-bitcode": true,
    "panic-strategy": "abort",
    "target-endian": "little",
    "target-pointer-width": "32",
    "target-c-int-width": "32",
    "merge-functions": "disabled"
}
